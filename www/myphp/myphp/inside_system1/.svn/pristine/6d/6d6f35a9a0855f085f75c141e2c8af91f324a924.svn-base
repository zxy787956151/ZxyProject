body
{
    text-align:center;
	font-family: 宋体;
	margin-top: 0px;
	font-size: 10pt;	
}
#logo
{
	background-image: url(/jw/Public/Images/logo1.jpg);
    background-repeat: no-repeat;
    width: 100%;
    height: 180px;
    text-align: right;	
}
#home
{
	position:relative;
	margin:0px auto;
	width:980px;
	text-align:left;
}
#menu
{
	height: 40px;
    margin-top: 0px;  
	padding-top:10px ;
	font-size: 12pt;
    font-weight:bold;	
}
#menu li
{   
	list-style-type: none;
	float: left;
}
#menu ul li ul{ 
    top: 45px;
}
#menu li ul { 
    line-height: 27px; 
    list-style-type: none;
    text-align:left; 
    left: -999em; 
    width: 120px; 
    position: absolute; 
} 
#menu li ul li{ 
    float: left; 
    width: 120px; 
    background: #8ae2ff; 
} 
#menu li ul a{ 
    display: block;
    width: 96px;
    text-align:left;
    padding-left:24px; 
} 
#menu li ul a:link { 
    color:#330000; 
    text-decoration:none; 
} 
#menu li ul a:visited { 
    color:#009cd0;
    text-decoration:none; 
} 
#menu li ul a:hover { 
    color:#009cd0;
    text-decoration:none;
    font-weight:bold; 
} 
#menu li:hover ul { 
    left:-15px; 
} 

#room
{
	margin-bottom:20px;	
}
#room span{
    float:right;
}
#left
{
    width: 300px;
    height: 270px;
    clear:left;
    float: left;
    overflow:hidden;     
    background-position: center  6%;
    background-image: url(/jw/Public/Images/left.jpg);
    background-repeat: no-repeat;
    background-color: #ececec;
    font-size: small;
    line-height: 18pt;
    padding-top: 40px;
    text-align: left;
}
#right
{
    width: 650px;
    height: 270px;
    clear:right;
    float: right;
    overflow:hidden;     
    background-image: url(/jw/Public/Images/right.jpg);
    background-repeat: no-repeat;
    font-size: small;
    line-height: 18pt;
    padding-top: 40px;
    text-align: left;
}
#buttom
{
	height:50px;
	font-family:黑体;
	font-weight:bold;
	color:red;
	font-size:11pt;
	clear:left;
}
#footer
{   
    height:100px;
	text-align:center;
	background-color:#ececec;
	padding-top:20px;
	margin-top:50px;
}
a
{
	text-decoration: none;
    color:#000;
}

#footer{
    padding-top:30px;
	padding-left:250px;
	font-size: 13pt;
	padding-left:250px;	
	line-height: 20pt;
}
#footer li{
    list-style: none;
	float: left;	
}